class Promotion
	types
    public String = seq of char;

	instance variables
		protected name:String; 
	  protected rewards:set of (Reward);
	  protected brand:Brand;

	operations
		public Promotion: String * set of Reward * Brand ==> Promotion
		Promotion(nam, sor, b)== 
		(
				name := nam;
				rewards := sor;
				brand := b;
		)

end Promotion
