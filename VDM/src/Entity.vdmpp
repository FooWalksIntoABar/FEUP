-- Represents an Entity
-- Contains the name of the Entity

class Entity 
	types 
		public String = seq of char;
		
	instance variables
	
	  -- the name of the Entity
		private name: String;
	  
	operations
		public Entity: String ==> Entity
		Entity(nam)==
		(
			name := nam;
		);
		
		-- Returns the name of the Entity
		pure public getName:() ==> String
		getName()==(return name);
		
end Entity
