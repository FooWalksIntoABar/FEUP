class ContainingProducts
	types 
			protected String = seq of char;

	instance variables
	  protected products: set of Product;
	  
	operations
		public ContainingProducts() res:ContainingProducts ==(products:={});
	
		public addProduct(prod: Product)==(products := products union {prod});
		
		public removeProduct(prod: Product)==(products := products \ {prod});
		
		pure public getProducts() res: set of Product ==(return products);

end ContainingProducts