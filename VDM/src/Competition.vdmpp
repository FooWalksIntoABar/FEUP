class Competition is subclass of Promotion			
	instance variables
		protected result:bool; 

	operations
		public Competition: String * set of Reward * Brand ==> Competition
		Competition(nam, sor, b)== 
		(
				name := nam;
				rewards := sor;
				brand := b;
				result := false;
		);
		
		pure public getResult:() ==> bool
	   getResult()==(return result);

end Competition